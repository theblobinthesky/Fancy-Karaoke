; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Fancy Karaoke"
run/main_scene="res://scenes/start_screen.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
boot_splash/image="res://Fancy-Karaoke_Logo.png"
config/icon="res://Fancy-Karaoke_Icon.png"

[autoload]

Globals="*res://script/Globals.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2

[dotnet]

project/assembly_name="Fancy Karaoke"

[rendering]

renderer/rendering_method="mobile"
